{
  "identifier": "gle.gemini.translator",
  "version": "0.0.6",
  "category": "translate",
  "name": "Gemini Translator",
  "summary": "Gemini powered translator",
  "icon": "",
  "author": "Brain <827421256@qq.com>",
  "minBobVersion": "0.5.0",
  "homepage": "https://github.com/Brain777777/bob-plugin-gemini",
  "appcast": "https://raw.githubusercontent.com/Brain777777/bob-plugin-gemini/main/appcast.json",
  "options": [
    {
      "identifier": "apiKeys",
      "type": "text",
      "title": "API KEY",
      "desc": "必填项",
      "textConfig": {
        "type": "secure",
        "height": "40",
        "placeholderText": "xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"
      }
    },
    {
      "identifier": "apiUrl",
      "type": "text",
      "title": "API URL",
      "defaultValue": "https://generativelanguage.googleapis.com",
      "desc": "可选项。如果您的网络环境需要代理才能访问 Gemini API, 可在这里修改为反向代理 API 的地址",
      "textConfig": {
        "type": "visible",
        "placeholderText": "https://generativelanguage.googleapis.com"
      }
    },
    {
      "identifier": "models",
      "type": "text",
      "title": "自定义模型",
      "defaultValue": "gemini-1.5-pro",
      "desc": "可选项。选择自己需要的模型",
      "textConfig": {
        "type": "visible",
        "placeholderText": "gemini-1.5-pro"
      }
    }
  ]
}
